AR=${HOME}/aarch64-rpi4-linux-gnu/bin/aarch64-rpi4-linux-gnu-ar
CC=${HOME}/aarch64-rpi4-linux-gnu/bin/aarch64-rpi4-linux-gnu-gcc
LD=${HOME}/aarch64-rpi4-linux-gnu/bin/aarch64-rpi4-linux-gnu-gcc
COMPILER=gcc
CFLAGS=--sysroot="${HOME}/aarch64-rpi4-linux-gnu/aarch64-rpi4-linux-gnu/sysroot" -B"${HOME}/aarch64-rpi4-linux-gnu/bin/aarch64-rpi4-linux-gnu-" -I../include -D__aarch64__ -D_XOPEN_SOURCE=600 -D_BSD_SOURCE -D_DEFAULT_SOURCE -std=gnu99 -pedantic -Wall -W -Wundef -Wendif-labels -Wshadow -Wpointer-arith -Wcast-align -Wcast-qual -Wwrite-strings -Wstrict-prototypes -Wmissing-prototypes -Wnested-externs -Winline -Wdisabled-optimization -fstrict-aliasing -O2 -pipe -Wno-parentheses -fPIC
PTHREAD_CFLAGS=-pthread
LDFLAGS=-fPIC
VMA_BITS=48
SSE=CK_MD_SSE_DISABLE
LSE=CK_MD_LSE_ENABLE
ALL_LIBS=libck.a
